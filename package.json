{
  "name": "devspace",
  "version": "1.0",
  "private": true,
  "description": "DevSpace monorepo",
  "scripts": {
    "dev:website": "cd apps/website && next dev",
    "dev:dashboard": "cd apps/dashboard && next dev",
    "dev:admin": "cd apps/admin && next dev",
    "dev": "concurrently \"npm run dev:website\" \"npm run dev:dashboard\" \"npm run dev:admin\"",
    
    "build:website": "cd apps/website && next build",
    "build:dashboard": "cd apps/dashboard && next build",
    "build:admin": "cd apps/admin && next build",
    "build": "npm run build:website && npm run build:dashboard && npm run build:admin",
    
    "start:website": "cd apps/website && next start",
    "start:dashboard": "cd apps/dashboard && next start",
    "start:admin": "cd apps/admin && next start",
    "start": "concurrently \"npm run start:website\" \"npm run start:dashboard\" \"npm run start:admin\"",
    
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.5.3",
    "turbo": "^1.12.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@nivo/bar": "^0.88.0",
    "@nivo/core": "^0.88.0",
    "@nivo/line": "^0.88.0",
    "@radix-ui/react-toast": "^1.2.7",
    "@supabase/supabase-js": "^2.49.4",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.6.5",
    "gsap": "^3.12.7",
    "lucide-react": "^0.487.0",
    "next": "^15.3.0",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-big-calendar": "^1.18.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.2.0",
    "recharts": "^2.15.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.3",
    "zod": "^3.24.2"
  }
}
